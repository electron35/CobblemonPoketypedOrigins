{
    "hidden": true,
    "type": "origins:multiple",
    "curr": {
        "type": "origins:resource",
        "min": 0,
        "max": 2,
        "start_value": 0
    },
    "signal": {
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "origins:resource",
                        "resource": "poketyped:psychic/stance_curr",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "/title @s actionbar {\"text\":\"Extrasensory\",\"underlined\":true,\"color\":\"dark_purple\"}"
                    }
                },
                {
                    "condition": {
                        "type": "origins:resource",
                        "resource": "poketyped:psychic/stance_curr",
                        "comparison": "==",
                        "compare_to": 1
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "/title @s actionbar {\"text\":\"Psyshock\",\"underlined\":true,\"color\":\"dark_purple\"}"
                    }
                },
                {
                    "condition": {
                        "type": "origins:resource",
                        "resource": "poketyped:psychic/stance_curr",
                        "comparison": "==",
                        "compare_to": 2
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "/title @s actionbar {\"text\":\"Teleport\",\"underlined\":true,\"color\":\"dark_purple\"}"
                    }
                }
            ]
        }
    }
}